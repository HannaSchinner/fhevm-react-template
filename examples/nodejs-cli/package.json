{
  "name": "nodejs-cli-fhevm",
  "version": "1.0.0",
  "description": "Node.js CLI tool demonstrating framework-agnostic FHEVM SDK usage",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "fhevm-cli": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "encrypt": "tsx src/index.ts encrypt",
    "decrypt": "tsx src/index.ts decrypt",
    "contract": "tsx src/index.ts contract"
  },
  "keywords": [
    "fhevm",
    "cli",
    "encryption",
    "blockchain",
    "zama"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "dependencies": {
    "@fhevm/sdk": "workspace:*",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "fhevmjs": "^0.5.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

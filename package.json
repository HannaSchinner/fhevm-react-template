{
  "name": "fhevm-sdk-monorepo",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential dApps with any framework - Built for Zama Bounty",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:sdk": "npm run build --workspace=packages/fhevm-sdk",
    "build:examples": "npm run build --workspace=examples/nextjs-music-royalty && npm run build --workspace=examples/react-basic",
    "build": "npm run build:sdk && npm run build:examples",
    "dev:nextjs": "npm run dev --workspace=examples/nextjs-music-royalty",
    "dev:react": "npm run dev --workspace=examples/react-basic",
    "dev:nodejs": "npm run start --workspace=examples/nodejs-cli",
    "test:sdk": "npm run test --workspace=packages/fhevm-sdk",
    "test:all": "npm run test --workspaces",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "rm -rf node_modules packages/*/node_modules examples/*/node_modules packages/*/dist examples/*/.next examples/*/dist",
    "compile:contracts": "cd examples/nextjs-music-royalty && npx hardhat compile"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "zama",
    "blockchain",
    "ethereum",
    "sdk",
    "react",
    "nextjs",
    "nodejs",
    "privacy",
    "confidential",
    "homomorphic",
    "music",
    "royalty"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fhevm-sdk-monorepo"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

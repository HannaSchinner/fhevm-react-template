{
  "name": "@fhevm/sdk",
  "version": "1.0.0",
  "description": "Universal SDK for building confidential dApps with FHEVM (Fully Homomorphic Encryption)",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc --module esnext --outDir dist/esm",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "zama",
    "blockchain",
    "ethereum",
    "sdk",
    "privacy",
    "confidential",
    "homomorphic",
    "wagmi-like",
    "framework-agnostic"
  ],
  "author": "FHEVM SDK Contributors",
  "license": "MIT",
  "peerDependencies": {
    "ethers": "^6.4.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "fhevmjs": "^0.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "ethers": "^6.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
